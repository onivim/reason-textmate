    "@reason-native/rely": "^1.3.1",
    "@opam/ppx_deriving_yojson": "*",
    "refmterr": "*",
    "ocaml": ">=4.7.0 <4.9.0",
    "reason-oniguruma": "^2.0.2",
    "reperf": "^1.3.0",
    "rench": "^1.9.1"
  },
  "devDependencies": {
    "ocaml": ">=4.7.0 <4.9.0",
    "@opam/merlin": "*",
    "@opam/odoc": "*"
